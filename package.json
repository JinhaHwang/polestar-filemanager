{
  "name": "polestar-filemanager",
  "version": "0.1.0",
  "private": false,
  "main": "./lib/index.js",
  "homepage": "https://github.com/JinhaHwang/polestar-filemanager",
  "license": "MIT",
  "author": "NKIA",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "story": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "clean": "rimraf lib",
    "build-lib": "yarn clean && cross-env NODE_ENV=production babel ./src --out-dir lib --copy-files",
    "prepublishOnly": "yarn build-lib"
  },
  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": "defaults"
          },
          "useBuiltIns": "usage",
          "corejs": "3"
        }
      ]
    ],
    "plugins": [
      [
        "transform-remove-console",
        {
          "exclude": [
            "error",
            "warn"
          ]
        }
      ],
      "react-hot-loader/babel",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-async-to-generator",
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": "css"
        }
      ]
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "antd": "^4.0.3",
    "axios": "^0.19.2",
    "camelcase": "^5.3.1",
    "classnames": "^2.2.6",
    "identity-obj-proxy": "3.0.0",
    "immutable": "^4.0.0-rc.12",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "paths.macro": "^2.0.2",
    "polestar-icons": "^0.9.62",
    "polestar-ui-kit": "^0.10.36",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-immutable": "^4.0.0",
    "redux-promise-middleware": "^6.1.2",
    "redux-promise-middleware-actions": "^3.0.1",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "redux-undo": "^1.0.0",
    "reselect": "^4.0.0",
    "resolve": "1.15.1",
    "rxjs": "^6.5.4",
    "semver": "7.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@hot-loader/react-dom": "^16.11.0",
    "@storybook/addon-actions": "^5.3.12",
    "@storybook/addon-knobs": "^5.3.13",
    "@storybook/addon-links": "^5.3.12",
    "@storybook/addon-storysource": "^5.3.13",
    "@storybook/addons": "^5.3.12",
    "@storybook/preset-ant-design": "^0.0.1",
    "@storybook/preset-create-react-app": "^2.0.0",
    "@storybook/react": "^5.3.12",
    "@svgr/webpack": "5.2.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^10.0.1",
    "@testing-library/user-event": "^10.0.0",
    "@types/jest": "^25.1.3",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "8.0.6",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-react-app": "^9.1.1",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.2",
    "css-loader": "3.4.2",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "file-loader": "6.0.0",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "4.0.0-beta.11",
    "jest": "25.1.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "25.1.0",
    "jest-watch-typeahead": "0.4.2",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.6.4",
    "postcss-flexbugs-fixes": "4.2.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.2",
    "react-app-polyfill": "^1.0.6",
    "react-dev-utils": "^10.1.0",
    "react-hot-loader": "^4.12.18",
    "react-scripts": "^3.3.1",
    "resolve-url-loader": "3.1.1",
    "rimraf": "^3.0.2",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "terser-webpack-plugin": "2.3.5",
    "ts-pnp": "1.1.6",
    "url-loader": "4.0.0",
    "webpack": "4.42.0",
    "webpack-dev-server": "3.10.3",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "5.0.0"
  }
}
